<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record id="product_targus_a" model="product.product">
            <field name="name">Targus Laptop 16" Bag</field>
            <field name="type">product</field>
            <field name="cost_method">average</field>
            <field name="valuation">real_time</field>
            <field name="standard_price">60</field>
            <field name="property_stock_account_input" ref="account.o_expense"/>
            <field name="property_stock_account_output" ref="account.o_income"/>
        </record>

        <record id="targus_stock_inventory" model="stock.inventory">
            <field name="name">Inventory Adjustment: Targus Laptop 16" Bag</field>
        </record>
        <record id="targus_stock_inventory_product_a" model="stock.inventory.line">
            <field name="product_id" ref="product_targus_a"/>
            <field name="product_uom_id" ref="product.product_uom_unit"/>
            <field name="inventory_id" ref="stock_card.targus_stock_inventory"/>
            <field name="product_qty">500.0</field>
            <field name="location_id" ref="stock.stock_location_14"/>
        </record>

        <function model="stock.inventory" name="action_done">
            <function eval="[[('id', '=', ref('stock_card.targus_stock_inventory'))]]"
                model="stock.inventory"
                name="search"/>
        </function>
    </data>
</openerp>
